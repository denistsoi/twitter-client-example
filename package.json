{
  "name": "twitter-feed",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.19.2",
    "express": "^4.17.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.4.0",
    "twit": "^2.2.11"
  },
  "scripts": {
    "dev": "run-p start:server start:app watch:styles",
    "start:server": "node ./server/index.js",
    "test:server": "jest server/tests/*.js",
    "build:styles": "postcss src/styles/tailwind.css -o src/styles/app.css",
    "watch:styles": "postcss src/styles/tailwind.css -o src/styles/app.css -w",
    "prestart": "npm run build:styles",
    "start:app": "react-scripts start",
    "build:app": "react-scripts build",
    "test:app": "react-scripts test",
    "eject:app": "react-scripts eject"
  },
  "proxy": "http://localhost:3001",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.7.4",
    "dotenv": "^8.2.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0",
    "supertest": "^4.0.2",
    "tailwindcss": "^1.2.0"
  }
}